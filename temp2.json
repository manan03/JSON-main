{
    "ALBTargetGroup": {
        "Type1": "AWS::ElasticLoadBalancingV2::TargetGroup",
        "Properties": {
            "HealthCheckIntervalSeconds": 30,
            "HealthCheckTimeoutSeconds": 5,
            "HealthyThresholdCount": 3,
            "Port": 80,
            "Protocol": "HTTP",
            "UnhealthyThresholdCount": 5,
            "VpcId": {
                "Ref": "VpcId"
            }
        }
    },
    "ApplicationLoadBalancer": {
        "Type1": "AWS::ElasticLoadBalancingV2::LoadBalancer",
        "Properties": {
            "Subnets": {
                "Ref": "Subnets"
            }
        }
    }
}